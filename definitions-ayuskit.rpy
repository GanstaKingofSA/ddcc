####### Transforms #######

define ayuskit_pantime = 0.5

transform ayuskit_tc(x=640, z=0.9):
    subpixel True
    yanchor 1.0
    on show:
        zoom z*0.95 alpha 0.0
        xcenter x
        ypos 1.2 yoffset -20
        easein 0.25 zoom z alpha 1.0 yoffset 0
    on replace:
        alpha 1.0
        parallel:
            easein 0.25 xcenter x zoom z
        parallel:
            easein 0.15 yoffset 0 ypos 1.2

transform ayuskit_tinstant(x=640, z=0.9):
    subpixel True
    zoom z alpha 1.0
    xcenter x
    yanchor 1.0 ypos 1.2 yoffset 0

transform ayuskit_tf(x=640, z=0.9):
    subpixel True
    yanchor 1.0
    on show:
        zoom z*0.95 alpha 0.0
        xcenter x
        ypos 1.2 yoffset -20
        easein 0.25 zoom z*1.05 alpha 1.0 yoffset 0
    on replace:
        alpha 1.0
        parallel:
            easein 0.25 xcenter x zoom z*1.05
        parallel:
            easein 0.15 yoffset 0 ypos 1.2

transform ayuskit_th(z=0.9):
    subpixel True
    transform_anchor True
    on hide:
        easein 0.25 zoom z*0.95 alpha 0.0 yoffset -20
